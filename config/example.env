# Example environment configuration for AI-Orchestration-Platform
# Copy this file to .env and update with your actual values

# Server configuration
SERVER_HOST=0.0.0.0
SERVER_PORT=8000
DEBUG=true
LOG_LEVEL=debug

# Database configuration
DB_TYPE=postgres
DB_HOST=database
DB_PORT=5432
DB_USER=aiplatform
DB_PASSWORD=aiplatform
DB_NAME=aiplatform

# Redis configuration
REDIS_HOST=redis
REDIS_PORT=6379
REDIS_DB=0

# JWT Authentication
JWT_SECRET=your-secret-key-here
JWT_EXPIRATION=24h

# AI Orchestrator integration
AI_ORCHESTRATOR_ENDPOINT=http://ai-orchestrator:8080/api
AI_ORCHESTRATOR_API_KEY=your-api-key-here

# Fast Agent integration
FAST_AGENT_ENDPOINT=http://fast-agent:9000/api
FAST_AGENT_API_KEY=your-api-key-here

# OpenAI integration (if needed)
OPENAI_API_KEY=your-openai-api-key-here

# Monitoring
ENABLE_PROMETHEUS=true
PROMETHEUS_PORT=9090

# Feature flags
FEATURE_ADVANCED_ROUTING=true
FEATURE_PERFORMANCE_MONITORING=true
FEATURE_AUTO_SCALING=false
FEATURE_MULTI_TENANCY=false

# CORS settings
CORS_ORIGINS=http://localhost:3000,http://localhost:8080
